{
  "version": 2,
  "builds": [
    {
      "src": "api/*.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ],
  "env": {
    "LETTA_API_TOKEN": "@letta-api-token",
    "LETTA_PROJECT_ID": "@letta-project-id",
    "LETTA_PROJECT_SLUG": "@letta-project-slug"
  }
}
