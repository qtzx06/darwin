it res.json();\n    return data.count;\n  } catch (e) {\n    // If this fails, just pretend we have 1337 hackers registered \ud83d\ude0e\n    return 1337;\n  }\n};\n\nconst CalHacksLanding: React.FC = () => {\n  const [rsvpCount, setRsvpCount] = useState<number | null>(null);\n\n  useEffect(() => {\n    // No infinite loops here, promise!\n    fetchRSVPCount().then(setRsvpCount);\n  }, []);\n\n  return (\n    <div\n      style={{\n        fontFamily: 'Inter, Arial, sans-serif',\n        background: 'linear-gradient(120deg, #4F8CFF 0%, #38F9D7 100%)',\n        minHeight: '100vh',\n        margin: 0,\n      }}\n    >\n      {/* Hero Section */}\n      <section style={{ paddingTop: 64, textAlign: \"center\", color: \"#fff\" }}>\n        <img\n          src=\"https://calhacks.io/logo.svg\"\n          alt=\"CalHacks logo\"\n          width={110}\n          style={{ marginBottom: 18 }}\n        />\n        <h1 style={{ fontSize: \"3rem\", fontWeight: 800, margin: \"0 0 14px\" }}>\n          {eventInfo.name} \ud83d\ude80\n        </h1>\n        <p style={{ fontSize: \"1.3rem\", margin: \"0 0 10px\" }}>\n          <span role=\"img\" aria-label=\"calendar\">\ud83d\udcc5</span> {eventInfo.date} &nbsp; | &nbsp;\n          <span role=\"img\" aria-label=\"location\">\ud83d\udccd</span> {eventInfo.location}\n        </p>\n        <p style={{ fontSize: \"1.1rem\", margin: \"0 0 28px\" }}>\n          {eventInfo.tagline}\n        </p>\n        <a\n          href=\"https://apply.calhacks.io\"\n          target=\"_blank\"\n          rel=\"noopener noreferrer\"\n          style={{\n            background: \"#FFD600\",\n            color: \"#222\",\n            textDecoration: \"none\",\n            padding: \"15px 35px\",\n            borderRadius: 10,\n            fontWeight: 700,\n            fontSize: \"1.25rem\",\n            boxShadow: \"0 8px 36px rgba(0,0,0,0.12)\",\n            transition: \"background 0.2s, transform 0.2s\",\n            display: \"inline-block\",\n          }